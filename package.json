{
  "name": "vscode-rproto",
  "publisher": "roj",
  "displayName": "RProto",
  "description": "Syntax highlighting for RProto",
  "version": "0.0.1",
  "repository": "https://github.com/roj1512/vscode-rproto.git",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rproto",
      "aliases": ["RProto", "rproto"],
      "extensions": [".rproto"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rproto",
      "scopeName": "source.rproto",
      "path": "./syntaxes/rproto.tmLanguage.json"
    }]
  }
}
